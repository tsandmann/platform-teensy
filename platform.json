{
  "name": "teensy-ts",
  "title": "Teensy",
  "description": "Teensy is a complete USB-based microcontroller development system, in a very small footprint, capable of implementing many types of projects. All programming is done via the USB port. No special programmer is needed, only a standard USB cable and a PC or Macintosh with a USB port.",
  "homepage": "https://www.pjrc.com/teensy",
  "license": "Apache-2.0",
  "keywords": [
      "dev-platform",
      "ARM",
      "AVR",
      "Cortex-M",
      "NXP Semiconductors",
      "Microchip"
  ],
  "engines": {
    "platformio": "^6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsandmann/platform-teensy.git"
  },
  "version": "4.17.0",
  "frameworks": {
    "arduino": {
      "package": "framework-arduinoteensy",
      "script": "builder/frameworks/arduino.py"
    },
    "mbed": {
      "package": "framework-mbed",
      "script": "builder/frameworks/mbed.py"
    },
    "zephyr": {
      "package": "framework-zephyr",
      "script": "builder/frameworks/zephyr.py"
    }
  },
  "packages": {
    "toolchain-atmelavr": {
      "type": "toolchain",
      "optional": false,
      "owner": "platformio",
      "version": "~1.50400.0",
      "optionalVersions": [">=1.60301.0,<1.80000.0"]
    },
    "toolchain-gccarmnoneeabi": {
      "type": "toolchain",
      "optional": false,
      "owner": "platformio",
      "version": "~1.70201.0"
    },
    "framework-arduinoteensy": {
      "type": "framework",
      "optional": true,
      "owner": "tsandmann",
      "version": "https://github.com/tsandmann/teensy-cores.git#no_warnings"
    },
    "framework-mbed": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~6.51506.0"
    },
    "framework-zephyr": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~2.20701.0"
    },
    "tool-teensy": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "<2"
    },
    "tool-jlink": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "^1.63208.0"
    },
    "toolchain-arm-cortexm-mac": {
      "type": "toolchain",
      "owner": "tsandmann",
      "version": "https://github.com/tsandmann/arm-cortexm-toolchain-mac.git#v12.2-3"
    },
    "toolchain-arm-cortexm-macos-arm64": {
      "type": "toolchain",
      "owner": "tsandmann",
      "version": "https://github.com/tsandmann/arm-cortexm-toolchain-macos-arm64.git#v12.2-3"
    },
    "toolchain-arm-cortexm-linux": {
      "type": "toolchain",
      "owner": "tsandmann",
      "version": "https://github.com/tsandmann/arm-cortexm-toolchain-linux.git#v12.2-3"
    },
    "toolchain-arm-cortexm-win64": {
      "type": "toolchain",
      "owner": "tsandmann",
      "version": "https://github.com/tsandmann/arm-cortexm-toolchain-win64.git#v12.2-3"
    }
  }
}
